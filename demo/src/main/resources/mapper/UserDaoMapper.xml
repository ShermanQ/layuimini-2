<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.Dao.UserDaoMapper">
    <resultMap id="BaseResultMap" type="com.example.demo.models.trainModel" >
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="mode_name" property="mode_name" jdbcType="VARCHAR" />
        <result column="people_name" property="people_name" jdbcType="VARCHAR" />
        <result column="phone" property="phone" jdbcType="INTEGER" />
        <result column="date" property="date" jdbcType="VARCHAR" />
        <result column="dataset_name" property="dataset_name" jdbcType="VARCHAR" />
        <result column="mail" property="mail" jdbcType="VARCHAR" />
        <result column="gpu_nums" property="gpu_nums" jdbcType="VARCHAR" />
        <result column="state" property="state" jdbcType="BIT" />
    </resultMap>
    <sql id="Base_Column_List" >
        id, mode_name, people_name, phone, date, dataset_name, mail,gpu_nums, state
    </sql>
    <select id="selctByModelname" resultType="com.example.demo.models.trainModel" parameterType="java.lang.String">
    select
    <include refid="Base_Column_List" />
    from traj.train
    where mode_name = #{mode_name,jdbcType=VARCHAR}
    </select>
    <select id="queryAll" resultType="com.example.demo.models.trainModel">
        select
        <include refid="Base_Column_List" />
        from traj.train
        limit #{page},#{limit}
    </select>

    <select id="count" resultType="long">
        select count(1)
        from traj.train
    </select>
</mapper>